package desingerPatternAbstractFactory;

public class TesteFactory {
	
	
	public static Carro montaCarro(String tipo) {
		CarroFactory carro = null;
		switch (tipo) {
		case "Luxo":
			carro= new CarroLuxoFactory();
			break;
	    case "Popular":
	    	carro= new CarroPopular();
			break;	

		default:
			break;
		}
		
	return	carro;
	}
	

}
